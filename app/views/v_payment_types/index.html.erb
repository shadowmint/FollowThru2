<h1>Listing v_payment_types</h1>

<table>
  <tr>
    <th>V user data</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @v_payment_types.each do |v_payment_type| %>
  <tr>
    <td><%= v_payment_type.v_user_data_id %></td>
    <td><%= v_payment_type.name %></td>
    <td><%= link_to 'Show', v_payment_type %></td>
    <td><%= link_to 'Edit', edit_v_payment_type_path(v_payment_type) %></td>
    <td><%= link_to 'Destroy', v_payment_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

