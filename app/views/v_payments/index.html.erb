<h1>Listing v_payments</h1>

<table>
  <tr>
    <th>V user data</th>
    <th>V pledge</th>
    <th>V project</th>
    <th>Amount</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @v_payments.each do |v_payment| %>
  <tr>
    <td><%= v_payment.v_user_data_id %></td>
    <td><%= v_payment.v_pledge_id %></td>
    <td><%= v_payment.v_project_id %></td>
    <td><%= v_payment.amount %></td>
    <td><%= v_payment.date %></td>
    <td><%= link_to 'Show', v_payment %></td>
    <td><%= link_to 'Edit', edit_v_payment_path(v_payment) %></td>
    <td><%= link_to 'Destroy', v_payment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New V payment', new_v_payment_path %>
